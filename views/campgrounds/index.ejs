<!DOCTYPE html>
<html lang="en">

<%- layout('layouts/boilerplate') %>
    <h1>All Campgrounds list</h1>
    <div>
        <a href="/campground/new"><b>Create a new campground</b></a>
    </div>
    <ul>
        <%for(let campground of campgrounds){%>
            <li>
                <a href="/campground/<%=campground._id%>">
                    <%= campground.title %>
                </a>
            </li>
            <% } %>
    </ul>